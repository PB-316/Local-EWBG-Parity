        SuperscriptBox[
         RowBox[{"mHsq", "[", 
          RowBox[{"\[Lambda]", ",", "\[Phi]"}], "]"}], "2"], 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"Log", "[", 
           FractionBox[
            RowBox[{"mHsq", "[", 
             RowBox[{"\[Lambda]", ",", "\[Phi]"}], "]"}], 
            SuperscriptBox["\[Mu]", "2"]], "]"}], "-", 
          FractionBox["3", "2"]}], ")"}]}], "+", 
       RowBox[{"3", 
        SuperscriptBox[
         RowBox[{"mXsq", "[", 
          RowBox[{"\[Lambda]", ",", "\[Phi]"}], "]"}], "2"], 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"Log", "[", 
           FractionBox[
            RowBox[{"mXsq", "[", 
             RowBox[{"\[Lambda]", ",", "\[Phi]"}], "]"}], 
            SuperscriptBox["\[Mu]", "2"]], "]"}], "-", 
          FractionBox["3", "2"]}], ")"}]}], "+", 
       RowBox[{"6", 
        SuperscriptBox[
         RowBox[{"mWsq", "[", "\[Phi]", "]"}], "2"], 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"Log", "[", 
           FractionBox[
            SuperscriptBox[
             RowBox[{"mWsq", "[", "\[Phi]", "]"}], "2"], 
            SuperscriptBox["\[Mu]", "2"]], "]"}], "-", 
          FractionBox["5", "6"]}], ")"}]}], "+", 
       RowBox[{"3", 
        SuperscriptBox[
         RowBox[{"mZsq", "[", "\[Phi]", "]"}], "2"], 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"Log", "[", 
           FractionBox[
            RowBox[{"mZsq", "[", "\[Phi]", "]"}], 
            SuperscriptBox["\[Mu]", "2"]], "]"}], "-", 
          FractionBox["5", "6"]}], ")"}]}], "-", 
       RowBox[{"12", 
        SuperscriptBox[
         RowBox[{"mtsq", "[", "\[Phi]", "]"}], "2"], 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"Log", "[", 
           FractionBox[
            RowBox[{"mtsq", "[", "\[Phi]", "]"}], 
            SuperscriptBox["\[Mu]", "2"]], "]"}], "-", 
          FractionBox["3", "2"]}], ")"}]}], "-", 
       RowBox[{"8", 
        SuperscriptBox[
         RowBox[{"mNsq", "[", "\[Phi]", "]"}], "2"], 
        RowBox[{"(", 
         RowBox[{"Log", "[", 
          RowBox[{
           FractionBox[
            RowBox[{"mNsq", "[", "\[Phi]", "]"}], 
            SuperscriptBox["\[Mu]", "2"]], "-", 
           FractionBox["3", "2"]}], "]"}], ")"}]}]}], ")"}]}], "//", 
    "Simplify"}]}], ";"}]], "Input",
 CellLabel->
  "In[2496]:=",ExpressionUUID->"16d569ca-7bf7-4c41-8304-babd17343459"],

