Cell["Fix the vev", "Section",ExpressionUUID->"de769cd0-02aa-473e-ba62-404cfa8ac5b1"],

Cell["\<\
Try the renormalization condition that fixes the vev at zero temperature.\
\>", "TextIndent",ExpressionUUID->"a85f225c-ea04-4318-b709-64ab70e0d4d3"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"B", "[", 
    RowBox[{"\[Lambda]_", ",", "\[Phi]_"}], "]"}], ":=", 
   RowBox[{
    FractionBox["1", 
     RowBox[{"64", 
      SuperscriptBox["\[Pi]", "2"], 
      SuperscriptBox["vR", "4"]}]], 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"6", 
       SuperscriptBox[
        RowBox[{"mWsq", "[", "vR", "]"}], "2"]}], "+", 
      RowBox[{"3", 
       SuperscriptBox[
        RowBox[{"mZsq", "[", "vR", "]"}], "2"]}], "+", 
      SuperscriptBox[
       RowBox[{"mHsq", "[", 
        RowBox[{"\[Lambda]", ",", "vR"}], "]"}], "2"], "-", 
      RowBox[{"12", 
       SuperscriptBox[
        RowBox[{"mtsq", "[", "vR", "]"}], "2"]}], "-", 
      RowBox[{"8", 
       SuperscriptBox[
        RowBox[{"mNsq", "[", "vR", "]"}], "2"]}]}], ")"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"V\[Prime]B", "[", 
    RowBox[{"\[Lambda]_", ",", "\[Phi]_"}], "]"}], ":=", 
   RowBox[{
    RowBox[{"2", 
     RowBox[{"B", "[", 
      RowBox[{"\[Lambda]", ",", "\[Phi]"}], "]"}], " ", 
     SuperscriptBox["vR", "2"], 
     SuperscriptBox["\[Phi]", "2"]}], "-", 
    RowBox[{
     FractionBox["3", "2"], 
     RowBox[{"B", "[", 
      RowBox[{"\[Lambda]", ",", "\[Phi]"}], "]"}], " ", 
     SuperscriptBox["\[Phi]", "4"]}], "+", 
    RowBox[{
     RowBox[{"B", "[", 
      RowBox[{"\[Lambda]", ",", "\[Phi]"}], "]"}], " ", 
     SuperscriptBox["\[Phi]", "4"], 
     RowBox[{"Log", "[", 
      FractionBox[
       SuperscriptBox["\[Phi]", "2"], 
       SuperscriptBox["vR", "2"]], "]"}]}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"V\[Prime]0T", "[", 
   RowBox[{"\[Lambda]_", ",", "\[Phi]_"}], "]"}], ":=", 
  RowBox[{
   RowBox[{"V\[Prime]tree", "[", 
    RowBox[{"\[Lambda]", ",", "\[Phi]"}], "]"}], "+", 
   RowBox[{"V\[Prime]B", "[", 
    RowBox[{"\[Lambda]", ",", "\[Phi]"}], "]"}], "-", 
   RowBox[{"(", 
    RowBox[{
     RowBox[{"V\[Prime]tree", "[", 
      RowBox[{"\[Lambda]", ",", 
       SuperscriptBox["10", 
        RowBox[{"-", "3"}]]}], "]"}], "+", 
     RowBox[{"V\[Prime]B", "[", 
      RowBox[{"\[Lambda]", ",", 
       SuperscriptBox["10", 
        RowBox[{"-", "3"}]]}], "]"}]}], ")"}]}]}]}], "Input",
