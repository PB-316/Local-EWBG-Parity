Cell["Mass spectrum", "Section",ExpressionUUID->"8a215512-0dba-48c3-b432-1dc4bb33e43a"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"mWsq", "[", "\[Phi]_", "]"}], ":=", 
   FractionBox[
    RowBox[{
     SuperscriptBox["g", "2"], 
     SuperscriptBox["\[Phi]", "2"]}], "4"]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"mZsq", "[", "\[Phi]_", "]"}], ":=", 
   FractionBox[
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       SuperscriptBox["g", "2"], "+", 
