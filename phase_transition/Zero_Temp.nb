Cell[BoxData[
 RowBox[{"\[Lambda]min", "=", 
  RowBox[{"Catch", "[", 
   RowBox[{"Do", "[", 
    RowBox[{
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"V\[Prime]0T", "[", 
         RowBox[{"\[Lambda]", ",", 
