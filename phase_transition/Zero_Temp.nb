  RowBox[{
   RowBox[{"mNsq", "[", "\[Phi]_", "]"}], ":=", 
   RowBox[{
    SuperscriptBox["yN", "2"], 
    FractionBox[
     SuperscriptBox["\[Phi]", "2"], "2"]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"mHsq", "[", 
    RowBox[{"\[Lambda]_", ",", "\[Phi]_"}], "]"}], ":=", 
   RowBox[{
    RowBox[{"3", "\[Lambda]", " ", 
