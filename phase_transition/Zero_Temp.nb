Cell["\<\
This code analysis the zero temperature effective potential, and investigate \
the bounded from below (BFB) condition.\
\>", "TextIndent",ExpressionUUID->"7f65c439-11af-4111-807f-4bca16b6b7cd"],

Cell[CellGroupData[{

Cell["Fixed parameters", "Section",ExpressionUUID->"707627f8-4e29-4c46-af47-15df597bd021"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"g", "=", "0.65"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"gX", "=", "0.432371"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"yt", "=", "0.7"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"vR", "=", 
   RowBox[{"3", "*", 
    SuperscriptBox["10", "5"]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"yN", "=", "0.65"}], ";"}]}], "Input",
