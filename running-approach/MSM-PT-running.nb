Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"EE", "[", "vR_", "]"}], ":=", 
    RowBox[{
     FractionBox["1", 
      RowBox[{"48", "\[Pi]"}]], 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"2", 
        SuperscriptBox[
         RowBox[{"g2", "[", "vR", "]"}], "3"]}], "+", 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{
          SuperscriptBox[
           RowBox[{"g2", "[", "vR", "]"}], "2"], "+", 
          SuperscriptBox[
           RowBox[{"g1", "[", "vR", "]"}], "2"]}], ")"}], 
        RowBox[{"3", "/", "2"}]]}], ")"}]}]}], ";"}], "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{"High", "-", 
